  "description": "GenTok",
  "private": true,
  "scripts": {
    "start": "react-scripts start --env=local",
    "build:surf": "env-cmd -f .env.surfinn react-scripts --max_old_space_size=4096 build",
    "build-dev": "env-cmd -f .env.development react-scripts --max_old_space_size=4096 build",
    "build-stg": "env-cmd -f .env.staging react-scripts --max_old_space_size=4096 build",
    "build-prd": "env-cmd -f .env.production react-scripts build",
    "eject": "react-scripts eject",
    "lint": "eslint --ext .ts,.tsx .",
    "lint:fix": "eslint --fix --ext .ts,.tsx .",
    "prettier": "prettier --write 'src/**/*.{js,jsx,ts,tsx}'",
    "clear-all": "rm -rf build node_modules",
    "gen": "npx surfinn-cli generate",
    "test:api": "react-scripts test --testPathPattern=services",
    "test:store": "react-scripts test --testPathPattern=models",
    "test": "react-scripts test",
    "storybook": "start-storybook -p 6006 -s public -h local.surfinn.kr ",
    "build-storybook": "build-storybook -s public",
    "deploy:dev": "sh ~/f2d-deploy.sh",
    "deploy:stg": "sh ~/f2d-deploy-staging.sh"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "prettier"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
